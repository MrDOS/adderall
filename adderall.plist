<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>adderall</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/adderall</string>
        </array>
        <key>StandardOutPath</key>
        <string>/var/log/adderall.log</string>
        <key>StandardErrorPath</key>
        <string>/var/log/adderall.err</string>
        <key>KeepAlive</key>
        <true />
    </dict>
</plist>
